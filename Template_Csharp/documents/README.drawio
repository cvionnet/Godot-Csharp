        <mxGraphModel dx="1374" dy="782" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="827" pageHeight="1169" math="0" shadow="0">
            <root>
                <mxCell id="P--lgTeDFmGbvyN0N5bu-0"/>
                <mxCell id="P--lgTeDFmGbvyN0N5bu-1" parent="P--lgTeDFmGbvyN0N5bu-0"/>
                <mxCell id="P--lgTeDFmGbvyN0N5bu-4" value="&lt;h1&gt;&lt;font color=&quot;#ff8000&quot;&gt;Items&lt;/font&gt;&lt;/h1&gt;&lt;div&gt;&lt;u&gt;How to change items appearence behaviour :&lt;/u&gt;&lt;/div&gt;&lt;div&gt;&lt;ul&gt;&lt;li&gt;each item scene offers several properties to play with (timers)&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;/div&gt;&lt;p&gt;&lt;u&gt;How to add a new item :&lt;/u&gt;&lt;/p&gt;&lt;p&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span&gt;in Items&amp;gt;ItemSprites, duplicate an existing item&lt;/span&gt;&lt;/li&gt;&lt;ul&gt;&lt;li&gt;&lt;span&gt;make its Godot properties unique&lt;/span&gt;&lt;/li&gt;&lt;li&gt;change the animationSprite&lt;/li&gt;&lt;/ul&gt;&lt;li&gt;in StateManager.cs, add a new action in the enum&amp;nbsp; &amp;nbsp;&quot;ItemsActionList&quot;&lt;/li&gt;&lt;li&gt;in ItemsBrain.cs, add properties for the new action in &lt;i&gt;Create_Item()&lt;/i&gt; method&lt;/li&gt;&lt;li&gt;in ItemGeneric.cs, check the onBodyCharacterShapeEntered() method send / call message to destination&lt;/li&gt;&lt;/ul&gt;&lt;ul&gt;&lt;li&gt;(for characters) in&amp;nbsp;CCharacterSpecific.cs :&lt;/li&gt;&lt;ul&gt;&lt;li&gt;code the action in Action_Item() method&lt;/li&gt;&lt;li&gt;code the end of the action in ActionEnded_Item() method&lt;/li&gt;&lt;/ul&gt;&lt;li&gt;(for platforms) in PlatformGeneric.cs&lt;/li&gt;&lt;ul&gt;&lt;li&gt;code the action in Action_Item() method&lt;/li&gt;&lt;li&gt;(not implemented yet) code the end of the action in ActionEnded_Item() method&lt;/li&gt;&lt;/ul&gt;&lt;li&gt;&lt;strike&gt;in each actor where the item applies :&lt;/strike&gt;&lt;/li&gt;&lt;ul&gt;&lt;li&gt;&lt;strike&gt;open the code file (eg : Player, Pnj, PlatformBrain ...)&lt;/strike&gt;&lt;/li&gt;&lt;li&gt;&lt;strike&gt;in the &quot;Item_Action&quot; method, code the action&lt;/strike&gt;&lt;/li&gt;&lt;li&gt;&lt;strike&gt;in the &quot;onItemActionTimer_Timeout&quot; method, reset values to be back to normal&lt;/strike&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;p&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;" parent="P--lgTeDFmGbvyN0N5bu-1" vertex="1">
                    <mxGeometry x="10" y="30" width="790" height="510" as="geometry"/>
                </mxCell>
                <mxCell id="YvPEwRHIKi5sSSpkGWVU-0" value="&lt;h1&gt;&lt;font color=&quot;#ff8000&quot;&gt;Characters&lt;/font&gt;&lt;/h1&gt;&lt;div&gt;&lt;div&gt;&lt;u&gt;the CCharacterSpecific.cs file :&lt;/u&gt;&lt;br&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;extends the CCharacter class and contains all properties and methods specific to a game&lt;/li&gt;&lt;li&gt;can be emptied when copying the template&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;ul&gt;&lt;ul&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;u&gt;How to add a new PNJ :&lt;/u&gt;&lt;/span&gt;&lt;br&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span&gt;in CharactersBrain, add properties for a new PNJ in&amp;nbsp;&lt;/span&gt;&lt;i&gt;Initialize_PNJ()&lt;/i&gt;&lt;span&gt; method&lt;/span&gt;&lt;br&gt;&lt;/li&gt;&lt;ul&gt;&lt;li&gt;&lt;span&gt;create a new &lt;i&gt;new CCharacterGeneric();&lt;/i&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span&gt;set property&amp;nbsp;&lt;/span&gt;&lt;i&gt;IsPlateformer&lt;/i&gt;&amp;nbsp;to false for a top-down game (true for a platformer)&lt;/li&gt;&lt;li&gt;to use streering, create a new&amp;nbsp;&lt;i&gt;new CSteering();&lt;/i&gt;&lt;span&gt;&amp;nbsp; &amp;nbsp;(has default values)&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;li&gt;add a &quot;Item_Action&quot; method to code items action&lt;br&gt;&lt;/li&gt;&lt;li&gt;&lt;span&gt;be sure that the new character belongs to a group called in ItemsGeneric &amp;gt; onBodyCharacterShapeEntered()&amp;nbsp; &amp;nbsp; (or add the new group in this method)&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;" parent="P--lgTeDFmGbvyN0N5bu-1" vertex="1">
                    <mxGeometry x="10" y="559" width="780" height="331" as="geometry"/>
                </mxCell>
                <mxCell id="xf8cmiu5wUFxnjfFVtlw-0" value="&lt;h1 style=&quot;font-style: normal&quot;&gt;&lt;font color=&quot;#ff8000&quot;&gt;State Machine&lt;/font&gt;&lt;/h1&gt;&lt;span style=&quot;font-style: normal&quot;&gt;&lt;u&gt;How to copy from PNJ template :&lt;/u&gt;&lt;/span&gt;&lt;br&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;create a &quot;states&quot; folder&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;copy all files from PNJ/states to new folder&amp;nbsp; (at least &quot;StateMachine_Pnj.cs&quot;, &quot;Spawn_Pnj.cs&quot; and &quot;Idle_Pnj.cs&quot;)&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;rename the suffix on all files (_Pnj.cs)&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;in each files, change the class name&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;in Godot, add new Node for each state&amp;nbsp; (at least&amp;nbsp; &amp;nbsp;State Machine &amp;gt; Spawn &amp;amp; Idle)&amp;nbsp; +&amp;nbsp; attach scripts&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;on&amp;nbsp;State Machine, select the default state, usualy Spawn (in Godot properties)&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;in StateMachine_.cs :&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;change the type in StateMachine_Core&amp;lt;XXX&amp;gt;&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;change the type in Init_StateMachine()&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;in each State :&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;change the type of the _rootNode&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;in Enter_State(), change type in &quot;typeof(Pnj)&quot; and &quot;pRootNode as Pnj;&quot;&lt;br&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-style: normal&quot;&gt;... then add the new logic&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;span style=&quot;font-style: normal&quot;&gt;&lt;br&gt;&lt;/span&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;dashed=1;fontStyle=2" parent="P--lgTeDFmGbvyN0N5bu-1" vertex="1">
